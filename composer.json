{
    "name": "apsonex/saas-utils",
    "authors": [
        {
            "name": "Apsonex Inc.",
            "email": "apsonexinc@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "hashids/hashids": "^4.1",
        "stevebauman/purify": "^4.0"
    },
    "require-dev": {
        "laravel/framework": "^9.0",
        "nunomaduro/collision": "^6.1",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Apsonex\\SaasUtils\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Apsonex\\SaasUtils\\Tests\\": "tests",
            "Apsonex\\SaasUtils\\Seeders\\": "seeders"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Apsonex\\SaasUtils\\SaasUtilsServiceProvider"
            ],
            "aliases": {
                "DiskProvider": "Apsonex\\SaasUtils\\Facades\\DiskProvider"
            }
        }
    },
    "repositories": [
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
